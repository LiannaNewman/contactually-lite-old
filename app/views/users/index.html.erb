<h1 style="margin-top: 100px;">Welcome to Contactually Lite!</h1>
  <div class="border-login_form-div">
    <h3>Existing User? Select Your Name Below.</h3>
      <div>
        <% @users.each do |user| %>
          <a class="btn btn-default" href="/users/#{user.id}" role="button"><%= user.name %></a>
        <% end %>
        </div>
    <h3>New User? Click Below.</h3>
      <div>
        <a class="btn btn-default" href="/users/new" role="button">New User</a>
      </div>
  </div>
